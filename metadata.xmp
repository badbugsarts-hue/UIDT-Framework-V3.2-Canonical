          <rdf:li>Lattice QCD</rdf:li>
          <rdf:li>Gamma Scaling</rdf:li>
          <rdf:li>Glueball Spectrum</rdf:li>
          <rdf:li>Quantum Field Theory</rdf:li>
          <rdf:li>Casimir Anomaly</rdf:li>
          <rdf:li>Hubble Tension</rdf:li>
        </rdf:Bag>
      </dc:subject>
      <dc:date>2025-12-01</dc:date>
      <dc:publisher>Zenodo / OSF Preprints</dc:publisher>
